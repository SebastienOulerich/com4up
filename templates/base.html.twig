<!DOCTYPE html>
<html lang="fr">
<!-- UTILITAIRES -->
<meta charset="utf-8">
<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<!-- DESCRIPTION -->
<title>Com4Up - {% block title %}{% endblock %}</title>
<meta name="description" content="Agence de communication digitale">
<meta name="keywords" content="Com4Up, Agence, communication, digital, web, entreprise, video">

<!-- CSS -->
<link type="text/css" rel="stylesheet" href={{asset("css/fonts.css")}} />
<link type="text/css" rel="stylesheet" href={{asset("css/fixe.css")}} />
{% block stylesheets %}{% endblock %}

<!-- FAVICON -->
<link rel="apple-touch-icon-precomposed" sizes="57x57" href="../images/favicon/apple-touch-icon-57x57.png" />
<link rel="apple-touch-icon-precomposed" sizes="114x114" href="../images/favicon/apple-touch-icon-114x114.png" />
<link rel="apple-touch-icon-precomposed" sizes="72x72" href="../images/favicon/apple-touch-icon-72x72.png" />
<link rel="apple-touch-icon-precomposed" sizes="144x144" href="../images/favicon/apple-touch-icon-144x144.png" />
<link rel="apple-touch-icon-precomposed" sizes="60x60" href="../images/favicon/apple-touch-icon-60x60.png" />
<link rel="apple-touch-icon-precomposed" sizes="120x120" href="../images/favicon/apple-touch-icon-120x120.png" />
<link rel="apple-touch-icon-precomposed" sizes="76x76" href="../images/favicon/apple-touch-icon-76x76.png" />
<link rel="apple-touch-icon-precomposed" sizes="152x152" href="../images/favicon/apple-touch-icon-152x152.png" />
<link rel="icon" type="image/png" href="../images/favicon/favicon-196x196.png" sizes="196x196" />
<link rel="icon" type="image/png" href="../images/favicon/favicon-96x96.png" sizes="96x96" />
<link rel="icon" type="image/png" href="../images/favicon/favicon-32x32.png" sizes="32x32" />
<link rel="icon" type="image/png" href="../images/favicon/favicon-16x16.png" sizes="16x16" />
<link rel="icon" type="image/png" href="../images/favicon/favicon-128.png" sizes="128x128" />
<meta name="application-name" content="&nbsp;" />
<meta name="msapplication-TileColor" content="../images/favicon/#FFFFFF" />
<meta name="msapplication-TileImage" content="../images/favicon/mstile-144x144.png" />
<meta name="msapplication-square70x70logo" content="../images/favicon/mstile-70x70.png" />
<meta name="msapplication-square150x150logo" content="../images/favicon/mstile-150x150.png" />
<meta name="msapplication-wide310x150logo" content="../images/favicon/mstile-310x150.png" />
<meta name="msapplication-square310x310logo" content="../images/favicon/mstile-310x310.png" />

<!-- POLICE -->
<link type="text/css" rel="stylesheet" href="../fonts/fontawesome-pro-5.4.2-web/css/all.css" />
</head>

<body>
  <header id="HeaderDesktop">
    <h1 id="logo"><span id="Com">Com</span><span id="Four">4</span><span id="Up">Up</span></h1>
    <nav>
      <ul id="NavDesktop">
        {% if app.request.attributes.get('_route') == "default" %}
        <li class="Current_page"><a href="{{ path('default') }}">Accueil</a></li>
        {% else %}
        <li><a href="{{ path('default') }}">Accueil</a></li>
        {% endif %}
        {% if app.request.attributes.get('_route') == "services" %}
        <li class="Current_page"><a href="{{ path('services') }}">Services</a></li>
        {% else %}
        <li><a href="{{ path('services') }}">Services</a></li>
        {% endif %}
        {% if app.request.attributes.get('_route') == "projets" %}
        <li class="Current_page"><a href="{{ path('projets') }}">Projets</a></li>
        {% else %}
        <li><a href="{{ path('projets') }}">Projets</a></li>
        {% endif %}
        {% if app.request.attributes.get('_route') == "equipe" %}
        <li class="Current_page"><a href="{{ path('equipe') }}">Equipe</a></li>
        {% else %}
        <li><a href="{{ path('equipe') }}">Equipe</a></li>
        {% endif %}
        {% if app.request.attributes.get('_route') == "a_propos" %}
        <li class="Current_page"><a href="{{ path('a_propos') }}">A propos</a></li>
        {% else %}
        <li><a href="{{ path('a_propos') }}">A propos</a></li>
        {% endif %}
        {% if app.request.attributes.get('_route') == "contact" %}
        <li class="Current_page"><a href="{{ path('contact') }}">Contact</a></li>
        {% else %}
        <li><a href="{{ path('contact') }}">Contact</a></li>
        {% endif %}

      </ul>
    </nav>
  </header>
<main>
  {% block body %}{% endblock %}
</main>
  <footer>
        <div id="Footer_info">
            <h2>Com4Up</h2>
            <p>Agence Web / Audiovisuel accompagnant votre activité vers le Numérique.</p>
        </div>
        <div id="Footer_contact">
            <h2>Contact</h2>
            <p>10 Rue des coquelicots, 57730 Valmont</p>
            <p>contact@com4up.fr</p>
            <p>06 51 27 71 62</p>
        </div>
        <div id="Footer_Social">
            <h2>Social</h2>
            <div id="Footer_Social_ico">
              <a href="https://www.facebook.com/Com4Up/"><i class="fab fa-facebook-square"></i></a>
              <a href=""><i class="fab fa-linkedin"></i></a>
              <a href="https://www.instagram.com/Com4Up/"><i class="fab fa-instagram"></i></a>
            </div>
        </div>
        <div id="Footer_Legal">
            <h2>Informations</h2>
            <p><a href="{{ path('a_propos') }}">A propos</a></p>
            <p><a href="{{ path('mentions_legales') }}">Mentions Légales</a></p>
            <p>Copyright</p>
        </div>
  </footer>
  {% block javascripts %}{% endblock %}
</body>

</html>
